namespace de.hftl.permissions

import de.hftl.lock.*
import de.hftl.user.*


transaction GrantUnlock {
    --> Lock lock // Lock on which the key can be used
    o DateTime expirationDate optional
    --> User issuer
    --> User grantee
}

transaction RevokeUnlock {
    --> LockKey lockKey // key to remove
    --> User issuer
    --> User revokee
}

transaction GrantOwner {
    --> User issuer
    --> User grantee
}

transaction RevokeOwner {
    --> User issuer
    --> User revokee
}

namespace de.hftl.permissions

import de.hftl.lock.*
import de.hftl.user.*


transaction GrantUnlock {
    --> Lock lock
    o DateTime expirationDate optional
    --> User grantee
}

transaction RevokeUnlock {
    --> LockKey lockKey
    --> User revokee
}

transaction GrantOwner {
    --> User grantee
}

transaction RevokeOwner {
    --> User revokee
}
